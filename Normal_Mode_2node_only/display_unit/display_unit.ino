	dist_step_03=min_dist+round((max_dist-min_dist)/5.0*3.0);
	dist_step_04=min_dist+round((max_dist-min_dist)/5.0*4.0);
	dist_step_05=min_dist+round((max_dist-min_dist)/5.0*5.0);

	// initialize label position data
  // those are manualy defined in the Photoshop/Photopea file, since its just easier to do it that way instead of guessing the numbers

	sensor[0].label_xpos=55;
	sensor[0].label_ypos=190;		


	sensor[1].label_xpos=160;		
	sensor[1].label_ypos=200;	
		

	sensor[2].label_xpos=260;		
	sensor[2].label_ypos=190;		
	
	tft.setTextSize(3); // Set text size, adjust as needed
	for(int i=0;i<NUMBER_OF_SENSORS;i++){
	// calculate string width
	itoa(sensor[i].measured_distance_cm, buffer, 10); // convert integer to C-style string
	sensor[i].label_width = getStringWidth(buffer);
	}
}

